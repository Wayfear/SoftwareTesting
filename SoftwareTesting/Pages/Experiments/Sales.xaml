<UserControl x:Class="SoftwareTesting.Pages.Experiments.TabPage1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab Layout="Tab" SelectedSource="/Pages/Experiments/Sales/File.xaml">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="Import" Source="/Pages/Experiments/Sales/File.xaml"/>
                <mui:Link DisplayName="Pie Charm" Source="/Pages/Experiments/Sales/PieChart.xaml"/>
                <mui:Link DisplayName="Data" Source="/Pages/Experiments/Sales/Data.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
